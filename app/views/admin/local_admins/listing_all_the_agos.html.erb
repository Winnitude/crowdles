<h2>Listing all the AGO</h2>
<table border="1" class="table">
  <thead>
  <tr>
    <th><strong> Email <strong></th>
    <th><strong> Ip Address <strong></th>
    <th><strong> Title <strong></th>
    <th><strong> First Name <strong></th>
    <th><strong> Last Name <strong></th>
    <th><strong> Country <strong></th>
    <!--<th><strong> User Role<strong></th>-->
    <th><strong> Action<strong></th>
  </tr>
  </thead>
  <% @AGOS.each do |ago| %>
      <%profile= ago.profile.nil? ? Profile.new : ago.profile%>
      <tr>
        <td><%= ago.email %></td>
        <td><%= ago.last_sign_in_ip %></td>
        <td><%= profile.title %></td>
        <td><%= profile.first_name %></td>
        <td><%= profile.last_name %></td>
        <td><%= fetch_county_name(ago) %></td>
        <!--<td> <%#= ago.role %>     </td>-->
        <td> <% if @already_having_mago.blank? %>
                <%= link_to "Change to MAGO",show_AGO_for_change_role_path(ago.id)%>
             <% else %>
                <%= link_to "Change to MAGO","", confirm: 'This country is already having MAGO', method: :get %>
             <%end%>
       </td>

      </tr>

  <% end %>
  </table>