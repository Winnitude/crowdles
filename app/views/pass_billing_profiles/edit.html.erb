<%= content_for :head do  %>
    <script type="text/javascript">

        jQuery(function(){
//            var profile_currency = new LiveValidation('platform_billing_profile_currency',{validMessage:" ",onlyOnSubmit:true});
//            profile_currency.add( Validate.Presence ,{failureMessage:" Can't be empty"});

            var fname = new LiveValidation('platform_billing_profile_first_name',{validMessage:" ",onlyOnSubmit:true});
            fname.add( Validate.Presence ,{failureMessage:" Can't be empty"});

            var last_name = new LiveValidation('platform_billing_profile_last_name',{validMessage:" ",onlyOnSubmit:true});
            last_name.add( Validate.Presence ,{failureMessage:" Can't be empty"});

            var company= new LiveValidation('platform_billing_profile_company_name',{validMessage:" ",onlyOnSubmit:true});
            company.add( Validate.Presence ,{failureMessage:" Can't be empty"});

            var email = new LiveValidation('platform_billing_profile_email',{validMessage:" ",onlyOnSubmit:true});
            email.add( Validate.Presence ,{failureMessage:" Can't be empty"});
            email.add( Validate.Email);

            var profile_telephone_number = new LiveValidation("platform_billing_profile_telephone_number",{validMessage:" ",onlyOnSubmit:true});
            profile_telephone_number.add( Validate.Presence ,{failureMessage:" Can't be empty"});
            profile_telephone_number.add( Validate.Format , { pattern: /[+][0-9]{2} [0-9]{10}/g, failureMessage: "Must be in form +xx xxxxxxxxxx" });

            var language= new LiveValidation('platform_billing_profile_language',{validMessage:" ",onlyOnSubmit:true});
            language.add( Validate.Presence ,{failureMessage:" Can't be empty"});

            var country= new LiveValidation('platform_billing_profile_country',{validMessage:" ",onlyOnSubmit:true});
            country.add( Validate.Presence ,{failureMessage:" Can't be empty"});

            var city= new LiveValidation('platform_billing_profile_city',{validMessage:" ",onlyOnSubmit:true});
            city.add( Validate.Presence ,{failureMessage:" Can't be empty"});

            var zip= new LiveValidation('platform_billing_profile_zip_code',{validMessage:" ",onlyOnSubmit:true});
            zip.add( Validate.Presence ,{failureMessage:" Can't be empty"});

            var s1= new LiveValidation('platform_billing_profile_street1',{validMessage:" ",onlyOnSubmit:true});
            s1.add( Validate.Presence ,{failureMessage:" Can't be empty"});

            var s2= new LiveValidation('platform_billing_profile_street2',{validMessage:" ",onlyOnSubmit:true});
            s2.add( Validate.Presence ,{failureMessage:" Can't be empty"});

            var state= new LiveValidation('platform_billing_profile_state',{validMessage:" ",onlyOnSubmit:true});
            state.add( Validate.Presence ,{failureMessage:" Can't be empty"});

            var contact_telephone_number = new LiveValidation("platform_billing_profile_contact_telephone_number",{validMessage:" ",onlyOnSubmit:true});
            contact_telephone_number.add( Validate.Format , { pattern: /[+][0-9]{2} [0-9]{10}/g, failureMessage: "Must be in form +xx xxxxxxxxxx" });




        });
    </script>
<% end %>