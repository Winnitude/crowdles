<h2>User Details</h2>
<%= form_for :user, :url => update_user_info_path, :html => { :class => "form-horizontal"} do |user| %>
    <%= render :partial => 'users/user_info',:locals => {:user => user} %>
    <%= render :partial => 'users/profile',:locals => {:user => user,:profile=> @user.profile} %>

    <div class="form-actions">
      <%= user.submit "Update User",:class =>"btn btn-primary"  %>
    </div>
<%end%>
<%= link_to 'Back', user_management_path %>